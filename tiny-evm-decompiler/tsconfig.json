{
  "compilerOptions": {
    "target": "es2015",
    "lib": [
      "dom",
      "ES5"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "rootDir": "./src",
    "outDir": "./dist",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react"
  },
  "include": ["./src/"],
  "exclude": ["out"]
}